------------------------day2_2-----------------------
import java.util.Scanner;
class add
{   
  public static void main(String arg[]) 
{
  int a,b,ans; 
  Scanner sc = new Scanner (System.in);
  System.out.println("enter values for A & B = ");
  a=sc.nextInt();
  b=sc.nextInt();
  ans=a+b;
  System.out.println(""+ans);
}  
}  
