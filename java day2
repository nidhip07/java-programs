-------------------------------day2_6-------------------------------
import java.util.Scanner;
class student
{ int id;
  String  name = new String();
  int sem;
  student(int i,int s)
{  id=i;
   sem=s;
}   
  public static void main(String arg[]) 
{
  student s1=new student (1,3);
  student s2=new student (2,4);
  System.out.println("s1 id is "+s1.id);
  System.out.println("s2 id is "+s2.id);
}  
}  
