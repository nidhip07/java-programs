------------------------------day2_5----------------------------------
import java.util.Scanner;
class student
{ int id;
  int sem;
  student()
{  id=0;
   sem=3;
}   
  public static void main(String arg[]) 
{
  student s1=new student ();
  student s2=new student ();
  System.out.println("s1 id is "+s1.id);
  System.out.println("s2 id is "+s2.id);
}  
}  
