-------------------------day2_3----------------------------
import java.util.Scanner;
class loop
{   
  public static void main(String arg[]) 
{
  int i,ans=0; 
  Scanner sc = new Scanner (System.in);
  for(i=3;i<=10;i++)
  {
    ans = ans+i;
  }  
  System.out.println(""+ans);
}
}  
